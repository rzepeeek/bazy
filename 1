# pkt 4
alter table postac add funckje varchar(150);
# pkt 5
update postac set funckja ... where ...
# pkt 6
# krok 1 dodanie odpowiedniej kolumny dla klucza obcego
alter table postac add column statek varchar(50) default null; #MOZNA BEZ COLUMN
# krok 2 - dodanie kluczba obecego 
alter table postac add foreign key (statek) references statek(nazwa_statku) on delete set null;
# pkt 7
update postac set statek=... where...;
